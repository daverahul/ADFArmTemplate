{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sourcetemp",
						"type": "DatasetReference"
					},
					"name": "sourcesales"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable1",
						"type": "DatasetReference"
					},
					"name": "sink"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDERNUMBER as short,",
				"          QUANTITYORDERED as short,",
				"          PRICEEACH as double,",
				"          ORDERLINENUMBER as short,",
				"          SALES as double,",
				"          ORDERDATE as timestamp,",
				"          STATUS as string,",
				"          QTR_ID as short,",
				"          MONTH_ID as short,",
				"          YEAR_ID as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcesales",
				"sourcesales filter(SALES >= 2000) ~> filter1",
				"filter1 aggregate(groupBy(YEAR_ID,",
				"          MONTH_ID),",
				"     TotalSales = sum(SALES)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     input(",
				"          YEAR_ID as string,",
				"          MONTH_ID as string,",
				"          TOTALSALES as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true) ~> sink"
			]
		}
	}
}